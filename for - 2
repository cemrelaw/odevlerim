#include <iostream>
using namespace std;

int main()
{
	int sayac = 0;
	for (int i = 0; i < 3; i ++)
	{	
		for (int j = 1; j < 6; j++)
		{
			cout << i << " ";
		}
		cout << endl;
		for (int k = 0; k < 5; k++)
		{		
			sayac++;	
			if (sayac > 10)
			{
				break;
			}
			cout << "* ";
		}  
		cout << endl;
	}
	cout << endl;

	return 0;
}
