#include <iostream>
using namespace std;

int main()
{
	int sayac = 0;
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 2; j++)
		{
			cout << "# " << "- ";
			sayac++;
			if (sayac % 2 == 0)
			{
				break;
			}
			else
			{
				cout << "# ";
			}
		}
		cout << endl;
	}
	return 0;
}
